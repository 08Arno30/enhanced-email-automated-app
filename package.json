{
  "scripts": {
    "client": "yarn --cwd client start",
    "server": "yarn --cwd server start",
    "build": "yarn install && yarn --cwd client build",
    "start": "concurrently --kill-others-on-fail \"yarn --cwd server start\" \"yarn --cwd client start\""
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "env-cmd": "^10.1.0",
    "react-icons": "^5.1.0"
  }
}
