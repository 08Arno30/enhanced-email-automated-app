{
  "homepage": "https://08Arno30.github.io/automated-app",
  "scripts": {
    "client": "yarn --cwd client start",
    "server": "yarn --cwd server start",
    "build": "yarn --cwd client build",
    "start": "concurrently --kill-others-on-fail \"yarn --cwd server start\" \"yarn --cwd client start\"",
    "predeploy": "yarn --cwd client build",
    "deploy": "gh-pages -d client/build"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1"
  }
}
